{
    "tasks": [
        {
            "label": "start server",
            "type": "shell",
            "isBackground": true,
            "command": "ccac -v | nsimdrv -gdb -port 49105 -p nsim_isa_family=av2hs ",
            "presentation": { "reveal": "silent" },
            "problemMatcher": [{
              "pattern": [{
                "regexp": ".",
                "file": 1,"line": 1,
                "column": 1,"message": 1
              }],
              "background": {
                  "activeOnStart": true,
                  "beginsPattern": { "regexp": "." },
                  "endsPattern": { "regexp": "." }
              },
            }]
        },
    ],
    "version": "2.0.0"
}